MODULE main 
VAR
  s : 0..16;

ASSIGN
  init(s) := 0;
  next(s) := case
    s = 0  : {1, 8, 9};
    s = 1  : 2;
    s = 2  : 3;
    s = 3  : 4;
    s = 4  : {5, 14};
    s = 5  : 6;
    s = 6  : 7;
    s = 7  : 0;
    s = 8  : 10;
    s = 9  : 4;
    s = 10 : 11;
    s = 11 : 12;
    s = 12 : 13;
    s = 13 : 0;
    s = 14 : 15;
    s = 15 : {3, 16};
    s = 16 : 0;
    TRUE   : s;
  esac;
